#include <stdio.h>

int main() {
    char source[100], destination[100];
    printf("Enter a string to copy: ");
    fgets(source, sizeof(source), stdin);
    
    int i = 0;
    while ((destination[i] = source[i]) != '\0')
        i++;
    
    printf("Copied string: %s", destination);
    
    return 0;
}
